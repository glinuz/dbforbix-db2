	    &lt;discovery time=&quot;120&quot; item=&quot;discovery.DBforBIX.config[tablespace,{$DSN}]&quot; names=&quot;TABLESPACE&quot;&gt;select DISTINCT tbsp_name from SYSIBMADM.TBSP_UTILIZATION&lt;/discovery&gt;       		&#13;
	   &lt;multiquery time=&quot;30&quot; items=&quot;tablespace.usedkb[%1]|tablespace.totalkb[%1]&quot; type=&quot;column&quot;&gt;SELECT tbsp_name as TABLESPACE, sum(tbsp_used_size_kb) as usedkb,sum(tbsp_total_size_kb) as totalkb FROM SYSIBMADM.TBSP_UTILIZATION GROUP BY tbsp_name&lt;/multiquery&gt;&#13;
	&lt;/server&gt;&#13;
&lt;/parms&gt;</params>
